alias lll="ls -alhTOe@"
alias ll="ls -alh"
alias rm="rm -i"
alias mv="mv -i"
alias cp="cp -i"
alias ls="ls -G"

export LC_CTYPE=en_US.UTF-8
export PATH="$HOME/bin:$PATH"

# zsh plugins
source ~/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh
source ~/.zsh/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
